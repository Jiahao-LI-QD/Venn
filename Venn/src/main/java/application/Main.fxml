<AnchorPane fx:id="anchor" onMouseEntered="#detectLeft" prefHeight="600.0" prefWidth="1070.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Circle id="Left Circle" fill="#30068f" layoutX="431.0" layoutY="373.0" opacity="0.38" radius="220.0" stroke="BLACK" strokeWidth="4.0" />
      <Circle id="Right Circle" fill="RED" layoutX="721.0" layoutY="373.0" opacity="0.39" radius="220.0" stroke="#4d3f3f" strokeWidth="4.0" />
      <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" layoutX="475.0" layoutY="21.0" onMouseClicked="#setTitle" prefHeight="58.0" prefWidth="215.0" text="Venn" textAlignment="CENTER">
        
         <font>
            <Font size="27.0" />
         </font></Label>
         
      <Button id="test" fx:id="submit" layoutX="198.0" layoutY="70.0" mnemonicParsing="false" onAction="#printOutput" text="Add" />
      <TextField fx:id="inputText" layoutX="20.0" layoutY="50.0" onKeyPressed="#handle" prefHeight="59.0" prefWidth="162.0" />
      <ListView fx:id="holder" layoutX="17.0" layoutY="120.0" onDragDetected="#detactDrag" onKeyPressed="#keyPressHolder" onMouseClicked="#menuHolder" prefHeight="184.0" prefWidth="176.0" />
      <ListView fx:id="left" layoutX="310.0" layoutY="186.0" onKeyPressed="#keyPressLeft" onMouseClicked="#menuLeft" onMouseEntered="#detectLeft" onMouseMoved="#points" prefHeight="372.0" prefWidth="202.0" />
      <ListView fx:id="middle" layoutX="539.0" layoutY="257.0" onKeyPressed="#keyPressMiddle" onMouseClicked="#menuMiddle" onMouseEntered="#detectMiddle" prefHeight="236.0" prefWidth="77.0" />
      <ListView fx:id="right" layoutX="641.0" layoutY="191.0" onKeyPressed="#keyPressRight" onMouseClicked="#menuRight" onMouseEntered="#detectRight" prefHeight="367.0" prefWidth="197.0" />
      <Label fx:id="leftLabel" layoutX="403.0" layoutY="123.0" text="Label" />
      <Label fx:id="rightLabel" layoutX="707.0" layoutY="123.0" text="Label" />
      <TextField fx:id="left_label_input" layoutX="17.0" layoutY="445.0" prefHeight="25.0" prefWidth="103.0" />
      <TextField fx:id="right_label_input" layoutX="17.0" layoutY="512.0" prefHeight="25.0" prefWidth="104.0" />
      <Text layoutX="24.0" layoutY="430.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Left Circle" />
      <Text layoutX="19.0" layoutY="499.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Right Circle" />
      <Button fx:id="left_label_button" layoutX="132.0" layoutY="445.0" mnemonicParsing="false" onAction="#setLeftLabel" text="Set Left" />
      <Button fx:id="right_label_button" layoutX="132.0" layoutY="512.0" mnemonicParsing="false" onAction="#setRightLabel" text="Set Right" /> 
      <Button fx:id="clearLeft" layoutX="386.0" layoutY="83.0" mnemonicParsing="false" onAction="#clearLeftSet" text="Clear Set" />
      <Button fx:id="clearRight" layoutX="690.0" layoutY="83.0" mnemonicParsing="false" onAction="#clearRightSet" text="Clear Set" />
      <ListView fx:id="decoyLeft" layoutX="218.0" layoutY="233.0" onMouseEntered="#detectLeft" opacity="0.0" prefHeight="276.0" prefWidth="92.0" />
      <ListView fx:id="decoyRight" layoutX="838.0" layoutY="233.0" onMouseEntered="#detectRight" opacity="0.0" prefHeight="276.0" prefWidth="92.0" />
      <Button fx:id="textExport" layoutX="537.0" layoutY="128.0" mnemonicParsing="false" onAction="#exportAsText" text="Export As Text" />
      <Button fx:id="snapshot" layoutX="550.0" layoutY="83.0" mnemonicParsing="false" onAction="#takeSnapshot" text="Snapshot" /> 

   </children>
</AnchorPane>
